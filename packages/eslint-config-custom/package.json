{
	"private": true,
	"name": "eslint-config-custom",
	"version": "0.0.1",
	"description": "A custom eslint config for weilage.dev",
	"main": "index.js",
	"license": "ISC",
	"scripts": {
		"lint:eslint": "eslint --cache .",
		"lint:prettier": "prettier --cache --check . --ignore-path=../../.prettierignore",
		"lint": "concurrently pnpm:lint:* --kill-others-on-fail",
		"format": "prettier --cache --write . --ignore-path=../../.prettierignore"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-turbo": "^1.9.3",
		"eslint-plugin-svelte": "^2.27.3",
		"eslint-plugin-unicorn": "^47.0.0"
	}
}
