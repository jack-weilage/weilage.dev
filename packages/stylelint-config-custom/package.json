{
	"private": true,
	"name": "stylelint-config-custom",
	"version": "0.0.1",
	"description": "A custom stylelint config for weilage.dev",
	"main": "index.js",
	"license": "ISC",
	"scripts": {
		"lint:eslint": "eslint --cache .",
		"lint:prettier": "prettier --cache --check . --ignore-path=../../.prettierignore",
		"lint": "concurrently pnpm:lint:* --kill-others-on-fail",
		"format": "prettier --cache --write . --ignore-path=../../.prettierignore"
	},
	"dependencies": {
		"postcss-html": "^1.5.0",
		"stylelint-no-unsupported-browser-features": "^6.1.0",
		"stylelint-order": "^6.0.3"
	}
}
